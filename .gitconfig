[core]
	editor = vim
	filemode = false
[alias]
	co = checkout
	br = branch
	cm = commit
	st = status -sb
	unstage = reset HEAD --
	last = log -1 HEAD
	l = log --pretty=oneline -n 20 --graph
[color]
	ui = auto
[color "diff"]
	meta = blue bold
	frag = magenta bold
	old = red bold
	new = green bold
[color "status"]
	added = green
	changed = yellow
	untracked = red
